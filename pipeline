pipeline
	{
	 agent any
		stages {
			stage ("BUild") {
					steps {
						echo "this is build"
						sh "git fetch git@github.com:BharathSukka/seven.git"
						sh "ls"
						sh "git log --oneline"
						}
					}
			stage ("Deploy") {
					steps {
						echo "this is for deploy"
						}
					}
			}	
	}

